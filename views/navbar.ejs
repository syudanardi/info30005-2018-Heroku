<body>





    <nav id="topNav" class="navbar navbar-default">

        <a class= "navbar-brand" href="/">
            <img src="/resource/logo_bg.png" width="55" height="55" alt="">
            <strong id="brandName">Quick Health </strong>
        </a>


        <div id="adminForm">



            <!--<span id="loginText" class="navbar-text text-right">-->
                <!--Login-->
            <!--</span>-->

            <span id="registrationText" class="navbar-text text-right">
                <a href="/registration_form" id="register-title"> Register </a>
            </span>


            <!--<a href="#" class="dropdown-toggle" data-toggle="dropdown"><b>Login</b></a>-->

            <span id="loginText" class="navbar-text text-right dropdown-toggle" data-toggle="dropdown">
                Login
            </span>


            <ul id="login-dp" class="dropdown-menu dropdown-menu-right">
                <li id="toggleText">
                    <div class="row">
                        <div class="col-md-12">
                            <div id="login-text">
                                <h5>Login</h5>
                            </div>
                            <form class="form" role="form" method="POST" action="/loginSubmit" accept-charset="UTF-8" id="login-nav">
                                <div class="form-group">
                                    <label class="sr-only" for="exampleInputEmail2">Email address</label>
                                    <input type='text' name='username' id='inputEmail' placeholder='Username' class="form-control" required autofocus>
                                </div>
                                <div class="form-group">
                                    <label class="sr-only" for="exampleInputPassword2">Password</label>
                                    <input type="password" name="password" class="form-control" id="exampleInputPassword2" placeholder="Password" required>
                                    <div class="help-block text-right"><a href="">Forget the password ?</a></div>
                                </div>
                                <div class="form-group">

                                    <input type="submit" value="Sign In" class="btn btn-primary btn-block">
                                </div>
                            </form>
                        </div>
                        <div id="register-here">
                            <div class="bottom text-center">
                                New to us ? <a href="/registration_form"><b>Register Here</b></a>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
